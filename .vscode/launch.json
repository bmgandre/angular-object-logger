{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome Debug",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceRoot}",
        "sourceMaps": true,
        "userDataDir": "${workspaceRoot}/.vscode/chrome",
        "runtimeExecutable": "/usr/bin/google-chrome-stable",
        "runtimeArgs": [
          "--disable-session-crashed-bubble"
        ]
      },
    ]
  }
  